{
  "name": "dashboard-web",
  "version": "0.1.0",
  "description": "SyncFlow dashboard - real-time monitoring for MERN apps",
  "type": "module",
  "include": [
    "src"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm:dev:vite\" \"pnpm:dev:server\"",
    "dev:vite": "vite",
   "dev:server": "tsx watch server/dev.ts",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "start": "tsx server/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "mongoose": "^8.20.1",
    "openai": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "mongodb-memory-server": "^9.1.5",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.2.0"
  }
}
